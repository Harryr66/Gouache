{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "crons": [
    {
      "path": "/api/cron/monthly-billing",
      "schedule": "0 0 1 * *"
    },
    {
      "path": "/api/cron/daily-campaign-reset",
      "schedule": "0 0 * * *"
    }
  ]
}
